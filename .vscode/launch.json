{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch",
            "command": "dotnet watch --no-hot-reload",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/src/Presentation",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "https://localhost:5000"
            }
        },
        {
            "name": ".NET Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Presentation/bin/Debug/net8.0/HealthMed.Presentation.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Presentation",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "https://localhost:5000"
            }
        }
    ]
}